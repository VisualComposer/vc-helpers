{
  "name": "vc-helpers",
  "version": "0.0.2",
  "description": "VC helpers",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VisualComposer/vc-helpers.git"
  },
  "keywords": [
    "boilerplate",
    "typescript"
  ],
  "author": "Visual Composer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VisualComposer/vc-helpers/issues"
  },
  "homepage": "https://github.com/VisualComposer/vc-helpers",
  "devDependencies": {
    "@types/jest": "28.1.1",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "26.5.3",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "28.1.0",
    "ts-jest": "28.0.4",
    "typescript": "4.7.3"
  },
  "files": [
    "lib/**/*"
  ]
}
